<?xml version="1.0"?>
<configuration>
  <system.diagnostics>
    <trace>
      <listeners>
        
        <add type="Shared.OneTimeTextWriterTraceListener, Shared, Version=1.0.0.0, Culture=neutral"
             name="Text" 
             initializeData="logs\log.txt" >
          <filter type="" />
        </add>
        
      </listeners>
    </trace>
    
  </system.diagnostics>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
